{
  "name": "aisnippets",
  "displayName": "AI-Snippets",
  "description": "Snippets para Aplicações Informáticas",
  "version": "1.1.26",
  "publisher": "NMSilva",
  "repository": {
    "type": "git",
    "url": "https://github.com/NMDSilva/snippets.git"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "start": "npm run open && npm run explorer && npm run compile",
    "compile": "vsce package",
    "open": "start chrome https://marketplace.visualstudio.com/manage/publishers/nmsilva",
    "explorer": "start explorer ."
  },
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/htmlsnippets.json"
      },
      {
        "language": "asp",
        "path": "./snippets/htmlsnippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascriptsnippets.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.7.0"
  }
}